{
  "parserOptions": {
      "ecmaVersion": 2019,
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true
      }
  },
  "env": {
      "browser": true,
      "node": true
  },
  "rules": {
      "semi": "error",
      "indent": ["error", 2],
      "max-len": ["error", {"code": 80}]
  },
  "extends": "eslint-config-google"
}
